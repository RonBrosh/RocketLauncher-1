<animated-vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable" >
        <vector
            android:name="vector"
            android:width="200dp"
            android:height="200dp"
            android:viewportWidth="200"
            android:viewportHeight="200">
            <group
                android:name="rocket"
                android:pivotX="100"
                android:pivotY="100"
                android:rotation="45"
                android:translateX="15"
                android:translateY="-15">
                <group android:name="body_left">
                    <path
                        android:name="wing"
                        android:pathData="M 80 160 C 70 160 60 170 60 170 M 60 170 C 50 150 70 130 75 120 L 80 160"
                        android:strokeWidth="6"
                        android:strokeColor="#8516ff"
                        android:strokeLineCap="round"
                        android:strokeLineJoin="round" />
                    <path
                        android:name="body"
                        android:pathData="M 94 165 L 80 160 C 80 160 60 50 100 25 M 94 155 L 80 150"
                        android:strokeWidth="6"
                        android:strokeColor="#ff6ad4"
                        android:strokeLineCap="round"
                        android:strokeLineJoin="round" />
                </group>
                <group
                    android:name="body_right"
                    android:pivotX="100"
                    android:pivotY="100"
                    android:scaleX="-1">
                    <path
                        android:name="wing_1"
                        android:pathData="M 80 160 C 70 160 60 170 60 170 M 60 170 C 50 150 70 130 75 120 L 80 160"
                        android:strokeWidth="6"
                        android:strokeColor="#8516ff"
                        android:strokeLineCap="round"
                        android:strokeLineJoin="round" />
                    <path
                        android:name="body_1"
                        android:pathData="M 94 165 L 80 160 C 80 160 60 50 100 25 M 94 155 L 80 150"
                        android:strokeWidth="6"
                        android:strokeColor="#ff6ad4"
                        android:strokeLineCap="round"
                        android:strokeLineJoin="round" />
                </group>
                <path
                    android:name="engine"
                    android:pathData="M 94 170 L 94 145 C 94 135 106 135 106 145 L 106 170 C 106 180 94 180 94 170"
                    android:strokeWidth="6"
                    android:strokeColor="#a06aff"
                    android:strokeLineCap="round"
                    android:strokeLineJoin="round" />
                <path
                    android:name="window"
                    android:pathData="M 90 70 C 90 55 110 55 110 70 M 90 70 C 90 85 110 85 110 70"
                    android:strokeWidth="6"
                    android:strokeColor="#a06aff"
                    android:strokeLineCap="round"
                    android:strokeLineJoin="round" />
                <group android:name="flames">
                    <path
                        android:name="left_flame"
                        android:pathData="M 80 175 L 80 200"
                        android:strokeWidth="6"
                        android:strokeColor="#ffabe7"
                        android:strokeLineCap="round"
                        android:strokeLineJoin="round" />
                    <path
                        android:name="right_flame"
                        android:pathData="M 120 175 L 120 200"
                        android:strokeWidth="6"
                        android:strokeColor="#ffabe7"
                        android:strokeLineCap="round"
                        android:strokeLineJoin="round" />
                    <path
                        android:name="center_flame"
                        android:pathData="M 100 190 L 100 220"
                        android:strokeWidth="7"
                        android:strokeColor="#a06aff"
                        android:strokeLineCap="round"
                        android:strokeLineJoin="round" />
                </group>
                <group android:name="flames_clip">
                    <path
                        android:name="left_flame_1"
                        android:pathData="M 80 175 L 80 200"
                        android:strokeWidth="6"
                        android:strokeColor="#ff6ad4"
                        android:strokeLineCap="round"
                        android:strokeLineJoin="round"
                        android:trimPathEnd="0.4" />
                    <path
                        android:name="right_flame_1"
                        android:pathData="M 120 175 L 120 200"
                        android:strokeWidth="6"
                        android:strokeColor="#ff6ad4"
                        android:strokeLineCap="round"
                        android:strokeLineJoin="round"
                        android:trimPathEnd="0.4" />
                    <path
                        android:name="center_flame_1"
                        android:pathData="M 100 190 L 100 220"
                        android:strokeWidth="7"
                        android:strokeColor="#8516ff"
                        android:strokeLineCap="round"
                        android:strokeLineJoin="round"
                        android:trimPathEnd="0.6" />
                </group>
            </group>
        </vector>
    </aapt:attr>
    <target android:name="left_flame_1">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:duration="300"
                android:interpolator="@android:interpolator/linear"
                android:propertyName="trimPathOffset"
                android:repeatCount="infinite"
                android:repeatMode="restart"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType" />
        </aapt:attr>
    </target>
    <target android:name="right_flame_1">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:duration="300"
                android:interpolator="@android:interpolator/linear"
                android:propertyName="trimPathOffset"
                android:repeatCount="infinite"
                android:repeatMode="restart"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType" />
        </aapt:attr>
    </target>
    <target android:name="center_flame_1">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:duration="400"
                android:interpolator="@android:interpolator/linear"
                android:propertyName="trimPathOffset"
                android:repeatCount="infinite"
                android:repeatMode="restart"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType" />
        </aapt:attr>
    </target>
</animated-vector>
